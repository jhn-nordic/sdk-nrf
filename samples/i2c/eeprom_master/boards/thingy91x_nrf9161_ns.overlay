/ {
	aliases {
		temp-sensor = &bme680;
		humidity-sensor = &bme680;
		pressure-sensor = &bme680;
	};
};

&i2c2 {
	bme680: bme680@76 {};
	eeprom: at24@7 {
		compatible = "atmel,at24";
		status = "okay";
		size = <256>;
		pagesize = <256>;
		address-width = < 16 >;
		timeout = < 100 >;
		reg = <0x7>;
	};
};
